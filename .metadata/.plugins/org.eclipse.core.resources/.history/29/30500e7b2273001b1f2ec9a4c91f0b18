if(False):
    print("This is true")
    
y = 5

if(y >= 0):
    print("The y value is positive")
else:
    print("The y value is negative")
    
z = 13 
if(z < 0):
    print("The z value is negative")
elif(z == 0):
    print("The z value is zero.")
else:
    print("The z value is positive")
    
    

    